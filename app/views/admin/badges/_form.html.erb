<%= form_with(model: badge, local: true) do |form| %>
  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div class="field">
    <%= form.label :icon_file %>
    <%= form.text_field :icon_file %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
